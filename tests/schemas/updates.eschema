type Status:
    required link name -> str:
        constraint unique

type Tag:
    required link name -> str:
        constraint unique

link annotated_status:
    link property note -> str

link weighted_tags:
    link property weight -> int

link annotated_tests:
    link property note -> str

type UpdateTest:
    required link name -> str
    link comment -> str

    # for testing singleton links
    link status -> Status
    link annotated_status -> Status

    # for testing links to sets
    link tags -> Tag:
        cardinality := '**'
    link weighted_tags -> Tag:
        cardinality := '**'

    # for testing links to sets of the same type as originator
    link related -> UpdateTest:
        cardinality := '**'
    link annotated_tests -> UpdateTest:
        cardinality := '**'
